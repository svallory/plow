{
    "plugins": [ "theme-api", "alerts", "anchorjs", "highlightjs", "include"],
    "pluginsConfig": {
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "ts",
                    "name": "Typescript"
                }
            ]
        },
        "anchorjs": {
            "icon": "#",
            "placement": "left",
            "visible": "always",
            "class": "anchor-target"
        }
    }
}
